apiVersion: 1

datasources:

  - name: 'Prometheus'
    type: 'prometheus'
    access: 'proxy'
    org_id: 1
    url: 'http://prometheus:9090'
    is_default: true
    version: 1
    editable: true

  - name: "AWS_Cloudwatch"
    type: cloudwatch
    jsonData:
      authType: keys
      defaultRegion: eu-west-1
    secureJsonData:
      accessKey: "AWS_ACCESS_KEY"
      secretKey: "AWS_SECRET_KEY"

